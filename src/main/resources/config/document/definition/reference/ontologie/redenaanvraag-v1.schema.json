      "enum": [
        { "code": "1", "name": "Tijdelijk contract is afgelopen" },
        { "code": "2", "name": "Zelf ontslag genomen" },
        { "code": "3", "name": "Ontslagen" },
        { "code": "4", "name": "Ontslagen bij een reorganisatie" },
        { "code": "5", "name": "Faillissement bedrijf" },
        { "code": "6", "name": "Ziekte" },
        { "code": "7", "name": "Andere reden" }
      ]
