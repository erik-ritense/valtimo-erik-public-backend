        { "code": "1", "name": "WW" },
        { "code": "2", "name": "Ziektewet" },
        { "code": "3", "name": "Wao/Wia" },
        { "code": "4", "name": "Wga" },
        { "code": "5", "name": "Andere uitkering" }
      ]
