        { "code": "1", "name": "Maximale duur uitkering bereikt" },
        { "code": "2", "name": "Arbeidsgeschikt verklaard" },
        { "code": "3", "name": "Opgelegde maatregel" },
        { "code": "4", "name": "Gedeeltelijk arbeidsgeschikt verklaard" },
        { "code": "5", "name": "Andere reden" }
      ]
